{
    "description": "Filter images transparently for kids",
    "manifest_version": 2,
    "name": "Wingman Jr.",
    "version": "0.9.8",
  
    "permissions": [
      "webRequest",
      "webRequestBlocking",
      "<all_urls>",
      "menus",
      "proxy"
    ],
  
    "background": {
      "scripts": [
          "tfjs_2.7.0.js",
          "background.js"
        ]
    },

    "browser_action": {
      "default_title" : "Wingman Jr.",
      "default_icon" : {
        "32" : "icons/wingman_icon_32.png"
      }
    },

    "content_security_policy": "script-src 'self' 'unsafe-eval';  object-src 'self' blob:; img-src blob: data:;"
  }